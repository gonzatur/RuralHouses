<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    
    
    <managed-bean>
        <managed-bean-name>bkLogin</managed-bean-name>
        <managed-bean-class>Backed.BkLogin</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
        
    <managed-bean>
        <managed-bean-name>bkAdmin</managed-bean-name>
        <managed-bean-class>Backed.BkAdmin</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>bkRegister</managed-bean-name>
        <managed-bean-class>Backed.BkRegister</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/administrador.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>error</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>mantenerReg</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>seguirReg</from-outcome>
            <to-view-id>/registro.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>buscar</from-outcome>
            <to-view-id>/search.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>stay</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>bkOwner</managed-bean-name>
        <managed-bean-class>Backed.bkOwner</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/ownerHouses.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>hola</from-outcome>
            <to-view-id>/ownerHouses.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>bkOpinion</managed-bean-name>
        <managed-bean-class>Backed.bkOpinion</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bkReserva</managed-bean-name>
        <managed-bean-class>Backed.bkReserva</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bkIndex</managed-bean-name>
        <managed-bean-class>Backed.bkIndex</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bkSearchEngine</managed-bean-name>
        <managed-bean-class>Backed.bkSearchEngine</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/search.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>alojamiento</from-outcome>
            <to-view-id>/house.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
